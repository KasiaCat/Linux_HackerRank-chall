awk '{
if ( $4 == "" )
    print "Not all scores are available for", $1;
    }'
	
	
------------------------------------------------
	
awk '{
if ( $2 < 50 || $3 < 50 || $4 < 50 )
    print $1, ": Fail";
    
    else
    print $1, ": Pass";
    }'

-----------------------------------------------

awk '!/A/ && !/a/'

-----------------------------------------------

awk 'function f(x){
    if (x <= 1 ) {
        return 1;
    } else
        return x * f(x - 1)
        }
        
        NR > 1 {
        print f($0)
        }'
		

-----------------------------------------------
	sed 's/^[A-Z]/./' | paste -s
	
-----------------------------------------------

	sed -e "s/ the / this /"
-----------------------------------------------

	sed 's/thy/your/gi'

	